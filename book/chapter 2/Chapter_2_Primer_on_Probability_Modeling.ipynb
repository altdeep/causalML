        "id": "x_v3os5No1Mv",
        "outputId": "842f455f-2de2-4200-f0a9-44099f2dfdc7"
      },
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "tensor([0., 0., 0., 0., 1., 1., 1., 0., 0., 1.])"
            ]
          },
          "metadata": {},
          "execution_count": 22
